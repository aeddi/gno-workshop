{
    "name": "Gno",
    "build": {
        "dockerfile": "Dockerfile",
        "args": {
            "GO_VERSION": "1.23",
            "GNO_COMMIT": "2ac57c5b38631514fcd8ab0c7ba7aee292af77dc",
            "GNOPLS_COMMIT": "445cd71185dc03e8562058a9be76d9ff04e60dbc",
            "VSCODE_EXT_VERSION": "1.2.0"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": ["/vscode-ext/gno.vsix"],
            "settings": {
                "gno.gnodev.flags": ["local", "-web-home", "/r/workshop/home"]
            }
        }
    },
    "forwardPorts": [8888, 26657],
    "portsAttributes": {
        "8888": {
            "label": "Gnodev Web",
            "onAutoForward": "silent"
        }
    },
    "containerEnv": {
        "SHELL": "/bin/bash"
    }
}
